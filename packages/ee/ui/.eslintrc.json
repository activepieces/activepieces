{
    "extends": ["../../../.eslintrc.json"],
    "ignorePatterns": ["!**/*"],
    "overrides": [
      {
        "files": ["*.ts"],
        "rules": {
          "@typescript-eslint/adjacent-overload-signatures": "off",
          "@typescript-eslint/no-non-null-assertion": "off"
        },
        "extends": [
          "plugin:prettier/recommended"
        ],
        "plugins": ["prettier"]
      }
    ]
  }