{
  "name": "pieces-folk",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/pieces/community/folk/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/pieces/community/folk",
        "tsConfig": "packages/pieces/community/folk/tsconfig.lib.json",
        "packageJson": "packages/pieces/community/folk/package.json",
        "main": "packages/pieces/community/folk/src/index.ts",
        "assets": [
          "packages/pieces/community/folk/*.md",
          {
            "input": "packages/pieces/community/folk/src/i18n",
            "output": "./src/i18n",
            "glob": "**/!(i18n.json)"
          }
        ],
        "buildableProjectDepsInPackageJsonType": "dependencies",
        "updateBuildableProjectDepsInPackageJson": true
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs pieces-folk {args.ver} {args.tag}",
      "dependsOn": [
        "build"
      ]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ]
    }
  },
  "tags": []
}